{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "stage-1",
    "react"
  ],
  "plugins": [
    "universal-import"
  ],
  "env": {
    "test": {
      "presets": [
        "es2015",
        "stage-1"
      ]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-constant-elements"
      ]
    }
  }
}