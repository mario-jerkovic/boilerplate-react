{
  "presets": [
    [
      "env",
      {
        "targets": {
          "ie": 11
        },
        "modules": false
      }
    ],
    "stage-1",
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "universal-import"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-constant-elements"
      ]
    }
  }
}