{
  "name": "user-backend",
  "version": "1.0.0",
  "description": "Boilerplate-React",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "cross-env NODE_ENV=test mocha --require ./src/test-utils --compilers js:babel-core/register './src/**/*.spec.js'",
    "watch:test": "npm run test -- --watch",
    "watch:dll": "nodemon package.json --exec \"node webpack/dll-script.js\"",
    "watch:development": "npm run build:dll && cross-env NODE_ENV=development webpack-dev-server --colors --progress --config webpack",
    "build:dll": "DEBUG=webpack-dll node webpack/dll-script.js",
    "build:production": "cross-env NODE_ENV=production webpack -p  --colors --progress --display-error-details --config webpack"
  },
